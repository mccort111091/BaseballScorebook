                        <label><input type="checkbox" />Strike 1</label>
                        <label><input type="checkbox" />Strike 2</label>
                        <label><input type="checkbox" />Strike 3</label>
                    </div>
                    <div class="col-lg-1"></div>
                </div><!-- END OF pitchCount -->
                <div class="col-lg-12"><!-- START OF result -->
                    <div class="col-lg-1"></div>
                    <div class="col-lg-10" style="text-align:center;" id="result">
                        <h3>Result</h3>
                        <label><input type="radio" />Strike Out</label>
                        <label><input type="radio" />Single</label>
                        <label><input type="radio" />Triple</label>
                        <br />
                        <label><input type="radio" />Walk</label>
                        <label><input type="radio" />Double</label>
                        <label><input type="radio" />Home Run</label>
                    </div>
                    <div class="col-lg-1"></div>
                </div><!-- END OF result -->
